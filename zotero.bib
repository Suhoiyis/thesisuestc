}

@article{hanMSRLNetMultiSourceFusion2025,
  title = {{{MSRLNet}}: {{A Multi-Source Fusion}} and {{Feedback Network}} for {{EEG Feature Recognition}} in {{ADHD}}},
  shorttitle = {{{MSRLNet}}},
  author = {Han, Qiulei and Song, Ze and Ye, Hongbiao and Sun, Yan and Zhao, Jian and Shi, Lijuan and Kuang, Zhejun},
  date = {2025-10-22},
  journaltitle = {Brain Sciences},
