@article{duDataAugmentationMethod2024,
  title = {A {{Data Augmentation Method}} for {{Motor Imagery EEG Signals Based}} on {{DCGAN-GP Network}}},
  author = {Du, Xiuli and Ding, Xiaohui and Xi, Meiling and Lv, Yana and Qiu, Shaoming and Liu, Qingli},
  date = {2024-04-12},
