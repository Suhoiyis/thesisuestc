}

@article{heDataAugmentationDeep2021,
  title = {Data {{Augmentation}} for {{Deep Neural Networks Model}} in {{EEG Classification Task}}: {{A Review}}},
  shorttitle = {Data {{Augmentation}} for {{Deep Neural Networks Model}} in {{EEG Classification Task}}},
  author = {He, Chao and Liu, Jialu and Zhu, Yuesheng and Du, Wencai},
