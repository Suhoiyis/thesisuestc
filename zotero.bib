@article{habashiGenerativeAdversarialNetworks2023,
  title = {Generative Adversarial Networks in {{EEG}} Analysis: An Overview},
  shorttitle = {Generative Adversarial Networks in {{EEG}} Analysis},
  author = {Habashi, Ahmed G. and Azab, Ahmed M. and Eldawlatly, Seif and Aly, Gamal M.},
  date = {2023-04-11},
  journaltitle = {Journal of NeuroEngineering and Rehabilitation},
